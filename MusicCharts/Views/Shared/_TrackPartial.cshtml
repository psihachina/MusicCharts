@model IEnumerable<Track>

<div class="track-group">
    @foreach (var track in Model)
    {
        <div>
            <p>
                <img id="@track.ID" class="track track-play" alt="" />
                <span style="font-weight:700"><a asp-controller="Music" asp-action="Info" asp-route-id="@track.ID">@track.Name</a></span> - @string.Join(", ", track.Singers.Select(x => x.Name))
            </p>
        </div>
    }
</div>